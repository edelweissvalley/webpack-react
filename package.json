{
  "name": "webpack-react",
  "version": "0.0.0",
  "description": "Стартовая сборка на react",
  "main": "index.js",
  "repository": "https://github.com/edelweissvalley/webpack-react",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "eslint": "eslint --ext .ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc -p ./ --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.pcss": [
      "stylelint --config .stylelintrc.json"
    ],
    "*.{ts,tsx,js}": [
      "npm run eslint"
    ]
  },
  "author": "Anton <antonmajmistov@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-nested": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react-hot-loader": "^4.13.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}

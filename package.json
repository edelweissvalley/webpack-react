{
  "name": "webpack-react",
  "version": "0.0.0",
  "description": "Стартовая сборка на react",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/edelweissvalley/webpack-react"
  },
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "eslint": "eslint --ext .ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc -p ./ --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.pcss": [
      "stylelint --config .stylelintrc.json"
    ],
    "*.{ts,tsx,js}": [
      "npm run eslint"
    ]
  },
  "author": "Anton <antonmajmistov@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.4",
    "eslint": "^8.6.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.1",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^7.0.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^3.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.6.2",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
